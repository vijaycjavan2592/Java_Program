package basic_Code;

public class Array {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] a = {10,20};
		
		System.out.println(a.length);
		
		int[] ab = new int[2];
		
		for(int i=a.length-1 ; i>=0 ; i-- ){
			
			for(int j=0 ; j<ab.length-1 ; j++){
				ab[j] = a[i];
			
		
		System.out.println(ab[j]);

			}
		}
	}

}
